{
  "name": "content-expose",
  "version": "0.1.4",
  "description": "A lightweight content editing devtools panel for React applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "react",
    "devtools",
    "content",
    "cms",
    "json-editor"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "react-rnd": "^10.0.0",
    "react-simple-code-editor": "^0.14.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-rnd": "^10.4.13",
    "react-simple-code-editor": "^0.14.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
